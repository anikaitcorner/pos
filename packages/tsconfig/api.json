{
    "$schema": "https://json.schemastore.org/tsconfig",
    "extends":"./base.json",
    "display": "Express API",
    "compilerOptions": {
        "module": "NodeNext",
        "moduleResolution": "NodeNext",
        "noEmit": true,
        "strict": true,
        "esModuleInterop": true
    },
    "include": ["src"],
    "exclude": ["node_modules"]
}